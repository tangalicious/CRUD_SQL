/*************************************************
Name: crud_StandardFeedInsert
Author: Chris Tang
Created: 8/06/2018
Description: Create (CRUD)

Note: Create new records by utlizing INSERT statements

**************************************************/

IF OBJECT_ID('Feed.crud_StandardFeedInsert') IS NOT NULL
BEGIN
	DROP PROC
		Feed.crud_StandardFeedInsert;
END; 
GO
CREATE PROC Feed.crud_StandardFeedInsert
	(
	@Id INT,
	@Name VARCHAR(max),
	@Description VARCHAR(max),
	@Creator VARCHAR(max),
	@BusinessOwner VARCHAR(max),
	@ExampleFile VARCHAR(max),
	@Type VARCHAR(max),
	@Note VARCHAR(max),
	@Active bit,
	@CreateDate datetime,
	@ModifiedDate datetime,
	@CreatedBy  VARCHAR(max),
	@ModifiedBy  VARCHAR(max),
	@ExpirationDate datetime,
	@Activate datetime,
	)
AS
	SET NOCOUNT ON
	SET XACT_ABORT ON
	
BEGIN TRANSACTION
	INSERT INTO Feed.StandardFeed
	(
			Name,
			Description,
			Creator,
			BusinessOwner,
			ExampleFile,
			Type,
			Note,
			Active,
			CreateDate,
			ModifiedDate,
			CreatedBy,
			ModifiedBy,
			ExpirationDate,
			Activate
	)
	VALUES
	(
			@Name,
			@Description,
			@Creator,
			@BusinessOwner,
			@ExampleFile,
			@Type,
			@Note,
			@Active,
			@CreateDate,
			@ModifiedDate,
			@CreatedBy,
			@ModifiedBy,
			@ExpirationDate,
			@Activate
	)
	
DECLARE @StandardFeed.Id Int;
Set @StandardFeed.Id = SCOPE_IDENTITY()
		SELECT Id = @Id,
			Name = @Name,
			Description = @Description,
			Creator = @Creator,
			BusinessOwner = @BusinessOwner,
			ExampleFile =  @ExampleFile,
			Type = @Type,
			Note = @Note,
			Active = @Active,
			CreateDate = @CreateDate,
			ModifiedDate = @ModifiedDate,
			CreatedBy = @CreatedBy,
			ModifiedBy = @ModifiedBy,
			ExpirationDate = @ExpirationDate,
			Activate = @Activate
		WHERE ID = @Id;
	END;
GO
